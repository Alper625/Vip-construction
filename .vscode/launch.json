// filepath: .vscode/launch.json
{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Next.js: debug server-side",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--inspect-brk", "node_modules/next/dist/bin/next", "dev"],
      "port": 9229,
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
    }
  ]
}
